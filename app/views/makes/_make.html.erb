<div id="<%= dom_id make %>">
  <p>
    <strong>Make name:</strong>
    <%= make.make_name %>
  </p>

  <p>
    <strong>Car name:</strong>
    <%= make.car_name %>
  </p>

  <p>
    <strong>Model:</strong>
    <%= make.model %>
  </p>

  <p>
    <strong>Details:</strong>
    <%= make.details %>
  </p>

  <p>
    <strong>Image url:</strong>
    <%= make.image_url %>
  </p>

</div>
